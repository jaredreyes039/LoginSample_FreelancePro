import axios from "axios"


export const get = (URL: string, opts: object) => {
	axios({ url: URL, method: "GET", ...opts })
		.then()
		.catch()
}

export const post = (URL: string, data: FormData, opts: object) => {
	axios({ url: URL, method: "POST", data: data, ...opts })
		.then()
		.catch()
}

